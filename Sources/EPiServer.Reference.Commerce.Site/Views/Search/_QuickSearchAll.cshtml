@using EPiServer.Reference.Commerce.Site.Features.Search.Models

@model CustomSearchResult
@{
    Layout = null;
}

@{ 
    if (Model.RedirectUrl != "")
    {
        <script>
            window.location.href = "@Model.RedirectUrl";
        </script>
    }
    else
    {
        if (Model.ProductViewModels != null)
        {
            Html.RenderPartial("_QuickSearch", Model.ProductViewModels);
        }

        if (Model.ContentResult != null)
        {
            Html.RenderPartial("_QuickSearchContent", Model.ContentResult);
        }

        if (Model.PdfResult != null)
        {
            Html.RenderPartial("_QuickSearchPDF", Model.PdfResult);
        }
    }
}